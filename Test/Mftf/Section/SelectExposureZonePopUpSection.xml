<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="SelectExposureZonePopUpSection">
        <element name="stateExposurePopupTitle" type="text" selector="//p[contains(@data-bind,'exposure zone')]"/>
        <element name="state" type="select" selector="select[name=exemption-zone]"/>
        <element name="proceedButton" type="button" selector="//div[@class='exemption-zone-select-view fieldset']//button[contains(@data-bind,'Proceed')]"/>
        <element name="closeButton" type="button" selector="//aside[contains(@class,'account-add-exemption-modal')]//div[@class='modal-inner-wrap']//button[@data-role='closeBtn']" timeout="30"/>
    </section>
</sections>
