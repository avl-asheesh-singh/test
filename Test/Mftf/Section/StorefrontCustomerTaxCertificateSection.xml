<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontCustomerTaxCertificateSection">
        <element name="title" type="text" selector="//div[@class='page-title-wrapper']//span[@data-ui-id='page-title-wrapper']" timeout="30"/>
